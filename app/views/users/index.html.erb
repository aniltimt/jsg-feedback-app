<p id="notice"><%= notice %></p>
<h1>Listing Users</h1>
  <table class="table table-bordered">
    <thead>
      <tr>
        <th>Username</th>
        <th>User Email</th>        
        <th>Created Date</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= user.created_at %></td>        
        <%#= link_to 'Show', feedback %><%#= link_to 'Edit', edit_feedback_path(feedback) %>
        <%# if session[:user_id] == 1 %>
        <%#= link_to 'Destroy', "/users/#{user.id}", method: :delete, data: { confirm: 'Are you sure?' } %>
        <%# end %>
      </tr>
      <% end %>
    </tbody>
  </table>

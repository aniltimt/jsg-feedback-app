<h1>JetSetGo Feedback App</h1>
<p>Find me in app/views/home/index.html.erb - Home#index</p>

<%= form_for(@feedback) do |f| %>
  <% if @feedback.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@feedback.errors.count, "error") %> prohibited this product from being saved:</h2>

      <ul>
      <% @feedback.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
   <div class="field">
    <%= f.label :feedback_text %><br>
    <%= f.text_field :feedback_text %>
  </div>
<%= f.file_field :video %>
  <div class="actions">
      <%= f.submit 'Upload Video',  data: { disable_with: 'Uploading' } %>
    
  </div>
<% end %>

